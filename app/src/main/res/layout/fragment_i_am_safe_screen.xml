<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">

    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearColumnellipseeleven"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStyleyellow_51">
        <ScrollView android:clipToPadding="false"
            android:fillViewport="true" android:gravity="start"
            android:id="@+id/scrollViewScrollview"
            android:layout_height="0dp" android:layout_weight="1" android:layout_width="match_parent">
            <LinearLayout android:gravity="center_horizontal"
                android:id="@+id/linearIamSafeScree"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="vertical">

                <ImageView
                    android:id="@+id/imageEllipseEleven"
                    android:layout_width="@dimen/_200pxh"
                    android:layout_height="@dimen/_200pxh"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginStart="@dimen/_30pxh"
                    android:layout_marginEnd="@dimen/_30pxh"
                    android:layout_marginTop="@dimen/_60pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_ellipse11"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_ellipse11" />

                <TextView android:ellipsize="end"
                    android:gravity="center_horizontal"
                    android:id="@+id/txtGvendemisiniz"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_30pxh"
                    android:layout_marginStart="@dimen/_30pxh"
                    android:layout_marginTop="@dimen/_50pxv"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@string/msg_g_vende_misiniz"
                    style="@style/txtMontserratromanbold36" tools:text="@string/msg_g_vende_misiniz"/>

                <TextView
                    android:id="@+id/txtDescription"
                    style="@style/txtMontserratromanbold20"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_30pxh"
                    android:layout_marginTop="@dimen/_50pxv"
                    android:layout_marginEnd="@dimen/_30pxh"
                    android:gravity="center_horizontal"
                    android:text="@string/msg_g_vendeyseniz"
                    tools:text="@string/msg_g_vendeyseniz" />


            </LinearLayout>
        </ScrollView>
    </LinearLayout>
</layout>
