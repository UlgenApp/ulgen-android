                        android:layout_marginBottom="@dimen/_29pxv"
                        android:layout_marginStart="@dimen/_45pxh"
                        android:layout_marginTop="@dimen/_30pxv"
                        android:layout_width="@dimen/_35pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_bu052"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_bu052"/>
                    <ImageView android:id="@+id/imageSettingTwo"
                        android:layout_height="@dimen/_50pxh"
                        android:layout_marginBottom="@dimen/_22pxv"
                        android:layout_marginEnd="@dimen/_51pxh"
                        android:layout_marginStart="@dimen/_43pxh"
                        android:layout_marginTop="@dimen/_23pxv"
                        android:layout_width="@dimen/_50pxh"
                        android:scaleType="fitXY"
                        imageRes="@{@drawable/img_setting2}"
