<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data/>
    <LinearLayout android:id="@+id/linearColumnarrowleft"
        android:layout_height="match_parent"
        android:layout_marginBottom="0dp"
        android:layout_marginEnd="0dp"
        android:layout_marginStart="0dp"
        android:layout_marginTop="0dp"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStyleyellow_50">
        <androidx.appcompat.widget.Toolbar android:gravity="start"
            android:id="@+id/toolbarToolbar"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_18pxh"
            android:layout_marginStart="@dimen/_18pxh"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <LinearLayout android:id="@+id/linearRowarrowleft"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">

                <ImageView android:id="@+id/imageDownload"
                    android:layout_height="@dimen/_41pxh"

                    android:layout_width="@dimen/_35pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_download"
                    android:layout_gravity="center"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_download"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearColumnifrenimiunut"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_29pxv"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStyleyellow_50">
            <FrameLayout android:id="@+id/frameStackifrenimiunut"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_247pxv"
                android:layout_marginEnd="@dimen/_29pxh"
                android:layout_marginStart="@dimen/_29pxh" android:layout_width="@dimen/_295pxh">
                <TextView android:ellipsize="end"
                    android:gravity="top|start"
                    android:id="@+id/txtIfrenimiunut"
                    android:layout_gravity="bottom|start"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="match_parent"
                    android:singleLine="false"
                    android:text="@string/msg_ifreni_mi_unut2"
                    style="@style/txtMontserratromanmedium30" tools:text="@string/msg_ifreni_mi_unut2"/>
                <ImageView android:id="@+id/imageForgotOne"
                    android:layout_gravity="top|center"
                    android:layout_height="@dimen/_217pxh"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_marginEnd="@dimen/_33pxh"
                    android:layout_marginStart="@dimen/_33pxh"
                    android:layout_width="@dimen/_229pxh"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_forgot1}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_forgot1"/>
            </FrameLayout>
            <TextView android:ellipsize="end"
                android:gravity="center_horizontal"
                android:id="@+id/txtEpostaadresin"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_29pxh"
                android:layout_marginStart="@dimen/_29pxh"
                android:layout_marginTop="@dimen/_12pxv"
                android:layout_width="match_parent"
                android:singleLine="false"
                android:text="@string/msg_e_posta_adresin"
                style="@style/txtMontserratromanmedium14_1" tools:text="@string/msg_e_posta_adresin"/>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearColumnemail"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_25pxh"
                android:layout_marginStart="@dimen/_29pxh"
                android:layout_marginTop="@dimen/_43pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStyleindigo_700_8ccornerRadius">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtEmail"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_19pxh"
                    android:layout_marginStart="@dimen/_19pxh"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@string/lbl_e_mail"
                    style="@style/txtNunitoregular18" tools:text="@string/lbl_e_mail"/>
                <EditText

                    android:ellipsize="end"
                    android:id="@+id/etEmailOne"
                    android:inputType="textEmailAddress"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_16pxv"
                    android:layout_marginEnd="@dimen/_18pxh"
                    android:layout_marginStart="@dimen/_18pxh"
                    android:layout_marginTop="@dimen/_3pxv"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_11pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_11pxh"
                    android:paddingTop="@dimen/_12pxv"
                    android:background="@drawable/underline"
                    android:singleLine="true"
                    android:textColorHint="@color/gray_50"
                    style="@style/etSolidRounded" tools:ignore="Autofill"/>
            </LinearLayout>
            <androidx.appcompat.widget.AppCompatButton
                android:ellipsize="end" android:gravity="center"
                android:id="@+id/btnBalantyGnder"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_29pxh"
                android:layout_marginStart="@dimen/_29pxh"
                android:layout_marginTop="@dimen/_58pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_17pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_17pxv"
                android:singleLine="true"
                android:text="@string/msg_ba_lant_y_g_nd"
                android:textAllCaps="false"
                style="@style/btnSolidRounded" tools:text="@string/msg_ba_lant_y_g_nd"/>
            <FrameLayout android:gravity="start"
                android:id="@+id/frameStackyada"
                android:layout_height="@dimen/_26pxv"
                android:layout_marginTop="@dimen/_81pxv" android:layout_width="match_parent">

                <TextView
                    android:id="@+id/txtYada"
                    style="@style/txtMontserratromanregular20"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_gravity="top|center"
                    android:layout_marginStart="@dimen/_181pxh"
                    android:layout_marginEnd="@dimen/_181pxh"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:text="@string/lbl_ya_da"
                    tools:text="@string/lbl_ya_da" />
                <TextView android:ellipsize="end"
                    android:gravity="top|start"
                    android:id="@+id/txtLanguage"
                    android:layout_gravity="bottom|start"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="match_parent"
                    android:singleLine="true" android:text="@string/msg"
                    style="@style/txtMontserratromanextrabold20" tools:text="@string/msg"/>
            </FrameLayout>
            <TextView android:ellipsize="end"
                android:id="@+id/txtGiriyap"
                android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_29pxh"
                android:layout_marginStart="@dimen/_29pxh"
                android:layout_marginTop="@dimen/_16pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@string/lbl_giri_yap2"
                style="@style/txtMontserratromanmedium20" tools:text="@string/lbl_giri_yap2"/>
        </LinearLayout>
    </LinearLayout>
</layout>
