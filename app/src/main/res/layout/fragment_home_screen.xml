<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">



    <LinearLayout
        android:id="@+id/linearColumndownload"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center_horizontal"
        android:orientation="vertical">

        <ScrollView
            android:id="@+id/scrollViewScrollview"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:clipToPadding="false"
            android:fillViewport="true"
            android:gravity="start">

            <FrameLayout
                android:id="@+id/frameHomepage"
                style="@style/groupStyleyellow_51"
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                    <androidx.appcompat.widget.Toolbar
                        android:id="@+id/toolbarToolbar"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="top|start"
                        android:layout_marginBottom="@dimen/_10pxv"
                        app:contentInsetStart="0dp">

                        <LinearLayout
                            android:id="@+id/linearRowdownload"
                            style="@style/groupStyleyellow_51"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <ImageView
                                android:id="@+id/imageDownload"
                                android:layout_width="@dimen/_35pxh"
                                android:layout_height="@dimen/_41pxh"
                                android:layout_marginStart="@dimen/_15pxh"
                                android:layout_marginTop="@dimen/_17pxv"
                                android:layout_marginBottom="@dimen/_69pxv"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_download"
                                tools:ignore="ContentDescription"
                                tools:src="@drawable/img_download" />

                            <LinearLayout
                                android:id="@+id/linearColumnlanguage"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="@dimen/_94pxh"
                                android:layout_marginTop="@dimen/_51pxv"
                                android:layout_marginEnd="@dimen/_13pxh"
                                android:layout_marginBottom="@dimen/_3pxv"
                                android:layout_weight="0.89"
                                android:gravity="start"
                                android:orientation="vertical">

                                <LinearLayout
                                    android:id="@+id/frameStacklanguage"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:orientation="vertical">

                                    <TextView
                                        android:id="@+id/txtLanguage"
                                        style="@style/txtNunitobold14"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginEnd="@dimen/_10pxh"
                                        android:ellipsize="end"
                                        android:singleLine="true"
                                        tools:text="Merhaba Ulgen!" />

                                    <ProgressBar
                                        android:id="@+id/progressBar"
                                        style="?android:attr/progressBarStyleHorizontal"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:indeterminate="false"
                                        android:progress="50"
                                        android:progressDrawable="@drawable/progress_bar" />


                                </LinearLayout>


                                <TextView
                                    android:id="@+id/txtLgenhesabdo"
                                    style="@style/txtMontserratromanregular12"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/_4pxv"
                                    android:layout_marginEnd="@dimen/_10pxh"
                                    android:ellipsize="end"
                                    android:singleLine="false"
                                    android:text="@string/msg_lgen_hesab_do"
                                    tools:text="@string/msg_lgen_hesab_do" />

                                <TextView
                                    android:id="@+id/txtHesabntamaml"
                                    style="@style/txtMontserratromanregular11"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/_1pxv"
                                    android:layout_marginEnd="@dimen/_10pxh"
                                    android:ellipsize="end"
                                    android:singleLine="false"
                                    android:text="@string/msg_hesab_n_tamaml"
                                    tools:text="@string/msg_hesab_n_tamaml" />
                            </LinearLayout>
                        </LinearLayout>
                    </androidx.appcompat.widget.Toolbar>

                    <LinearLayout
                        android:id="@+id/linearColumnone"
                        style="@style/groupStyleindigo_700_cc"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_gravity="start"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:orientation="vertical">

                        <LinearLayout
                            android:id="@+id/linearColumnkullancgenel"
                            style="@style/groupStyleyellow_51cornerRadius"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_20pxh"
                            android:layout_marginTop="@dimen/_30pxv"
                            android:layout_marginEnd="@dimen/_20pxh"
                            android:gravity="center_horizontal"
                            android:layout_weight="1"
                            android:orientation="vertical">

                            <TextView
                                android:id="@+id/txtKullancgenelHeader"
                                style="@style/txtNunitoSansregular12Bold"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_margin="@dimen/_10pxh"
                                android:text="@string/lbl_dowload_and_forget_header" />

                            <TextView
                                android:id="@+id/txtKullancgenelBody"
                                style="@style/txtNunitoSansregular12"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:ellipsize="end"
                                android:gravity="start|center_vertical"
                                android:maxLines="10"
                                android:layout_margin="@dimen/_10pxh"
                                android:text="@string/lbl_dowload_and_forget_body" />
                        </LinearLayout>

                        <LinearLayout
                            style="@style/groupStyleyellow_51cornerRadius"
                            android:id="@+id/linearRowdeprembilgilen"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_20pxh"
                            android:layout_marginTop="@dimen/_20pxv"
                            android:layout_marginEnd="@dimen/_20pxh"
                            android:layout_marginBottom="@dimen/_20pxv"
                            android:layout_weight="1"
                            android:orientation="vertical">

                            <TextView
                                android:id="@+id/txtRotaVeYardimHeader"
                                style="@style/txtNunitoSansregular12Bold"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_margin="@dimen/_10pxh"
                                android:text="@string/lbl_route_and_help_header" />

                            <TextView
                                android:id="@+id/txtRotaVeYardimBody"
                                style="@style/txtNunitoSansregular12"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:ellipsize="end"
                                android:gravity="start|center_vertical"
                                android:maxLines="10"
                                android:layout_margin="@dimen/_10pxh"
                                android:text="@string/lbl_route_and_help_body" />
                        </LinearLayout>



                    </LinearLayout>

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/linearColumnblankprofilep"
                    style="@style/groupStyleyellow_1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="top|start"
                    android:layout_marginStart="@dimen/_26pxh"
                    android:layout_marginTop="@dimen/_50pxv"
                    android:layout_marginEnd="@dimen/_26pxh"
                    android:layout_marginBottom="@dimen/_50pxv"
                    android:orientation="vertical">

                    <de.hdodenhof.circleimageview.CircleImageView
                        android:id="@+id/imageBlankprofilep"
                        android:layout_width="@dimen/_86pxh"
                        android:layout_height="@dimen/_84pxh"
                        android:layout_gravity="center_horizontal"
                        android:layout_marginStart="@dimen/_7pxh"
                        android:layout_marginTop="@dimen/_7pxv"
                        android:layout_marginEnd="@dimen/_7pxh"
                        android:layout_marginBottom="@dimen/_6pxv"
                        android:scaleType="centerCrop"
                        android:src="@drawable/img_blankprofilep"
                        tools:ignore="ContentDescription"
                        tools:src="@drawable/thumbs_example" />
                </LinearLayout>
            </FrameLayout>
        </ScrollView>
    </LinearLayout>
</layout>
